{
  "name": "template-vite-vue-ts",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "start:dev": "vite --mode development",
    "start:test": "vite --mode testing",
    "start:staging": "vite --mode staging",
    "start:prod": "vite --mode production",
    "build": "pnpm type-check && vite build --mode production",
    "build:dev": "pnpm type-check && vite build --mode development",
    "build:test": "pnpm type-check && vite build --mode testing",
    "build:staging": "pnpm type-check && vite build --mode staging",
    "build:prod": "pnpm type-check && vite build --mode production",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "commit": "git add . && git cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "commitlint": "^17.4.2",
    "cz-git": "^1.4.1",
    "eslint": "^8.33.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.58.0",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.5",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.24"
  }
}
